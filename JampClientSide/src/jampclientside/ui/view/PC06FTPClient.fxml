<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="ftpPane" fillWidth="false" prefHeight="540.0" prefWidth="640.0" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jampclientside.ui.controller.PC06FTPClientController">
   <children>
      <Label alignment="TOP_LEFT" text="Cliente FTP" textAlignment="CENTER" textFill="#005192">
         <font>
            <Font name="Alef Bold" size="36.0" />
         </font>
      </Label>
      <HBox prefHeight="19.0" prefWidth="639.0" spacing="20.0">
         <children>
            <Button fx:id="btnUpload" mnemonicParsing="false" text="Subir Archivo" />
            <Button fx:id="btnDownload" mnemonicParsing="false" text="Descargar Archivo" />
            <Button fx:id="btnDeleteFile" alignment="CENTER_RIGHT" mnemonicParsing="false" style="-fx-background-color: red;" text="Eliminar Archivo" textAlignment="CENTER" textFill="WHITE">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox prefHeight="19.0" prefWidth="639.0" spacing="20.0">
         <children>
            <Button fx:id="btnCreateDir" mnemonicParsing="false" text="Crear Directorio" />
            <Button fx:id="btnDeleteDir" mnemonicParsing="false" style="-fx-background-color: red;" text="Eliminar Directorio" textFill="WHITE" />
         </children>
      </HBox>
      <TreeView fx:id="tView" prefHeight="301.0" prefWidth="480.0" />
      <HBox alignment="CENTER_LEFT" prefHeight="25.0" prefWidth="745.0" spacing="10.0">
         <children>
            <Label fx:id="lblInfo" minWidth="-Infinity" prefHeight="23.0" prefWidth="601.0" text="Mensaje Informativo" textFill="#31c300" />
         </children>
      </HBox>
      <HBox alignment="BOTTOM_RIGHT" prefHeight="25.0" prefWidth="724.0" spacing="10.0">
         <children>
            <Button fx:id="btnOut" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Salir" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
